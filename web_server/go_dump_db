. .env; 
docker-compose exec webserver_mysql mysql -u root -p$MYSQL_ROOT_PASSWORD -e "USE dbctf; \
show tables; \
select * from users; \
select * from flags; \
select * from ctfs; \
select * from logs; \
select * from feedbacks; \
SELECT COLUMN_NAME FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_NAME = 'flags'; \
select * from ctfsusers; \
"
